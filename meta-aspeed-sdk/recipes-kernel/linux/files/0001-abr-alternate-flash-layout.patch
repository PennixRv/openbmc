From 475791450752f076f47cd2917b751fdc686cfbca Mon Sep 17 00:00:00 2001
From: Jamin Lin <jamin_lin@aspeedtech.com>
Date: Mon, 2 Oct 2023 18:14:53 +0800
Subject: [PATCH] dts: update flash partition to support ABR mode

Signed-off-by: Jamin Lin <jamin_lin@aspeedtech.com>
---
 arch/arm/boot/dts/aspeed-ast2600-evb.dts | 3 ++-
 1 file changed, 2 insertions(+), 1 deletion(-)

diff --git a/arch/arm/boot/dts/aspeed-ast2600-evb.dts b/arch/arm/boot/dts/aspeed-ast2600-evb.dts
index a5536232d7c6..cae651de9559 100644
--- a/arch/arm/boot/dts/aspeed-ast2600-evb.dts
+++ b/arch/arm/boot/dts/aspeed-ast2600-evb.dts
@@ -257,10 +257,11 @@ flash@0 {
 	flash@1 {
 		status = "okay";
 		m25p,fast-read;
-		label = "fmc0:1";
+		label = "alt-bmc";
 		spi-rx-bus-width = <4>;
 		spi-tx-bus-width = <4>;
 		spi-max-frequency = <50000000>;
+#include "aspeed-evb-flash-layout-64-alt.dtsi"
 	};
 
 	flash@2 {
-- 
